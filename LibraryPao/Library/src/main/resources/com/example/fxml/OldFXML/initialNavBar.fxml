<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefHeight="1080.0" prefWidth="1920.0" styleClass="hidden-scrollbar" vbarPolicy="NEVER" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.initialNavBar">
   <content>
      <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="480.0" minWidth="680.0" prefHeight="1180.0" prefWidth="1920.0" stylesheets="@styles.css">
   <top>
      <StackPane alignment="TOP_CENTER" prefHeight="200.0" BorderPane.alignment="TOP_CENTER">
         <children>
            <ImageView fitHeight="300.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/Header.png" />
               </image>
            </ImageView>
            <Pane styleClass="semi-transparent-green" />
            <Pane maxHeight="-Infinity" prefHeight="80.0" styleClass="semi-transparent-black" />
            <VBox alignment="TOP_CENTER">
               <children>
                  <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="80.0" spacing="50.0">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
                           <image>
                              <Image url="@images/Logo.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </ImageView>
                        <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="homeButton" mnemonicParsing="false" onAction="#returnHome" styleClass="transparent-button" text="Home">
                           <font>
                              <Font name="Lucida Sans Unicode" size="30.0" />
                           </font>
                        </Button>
                        <Button fx:id="aboutButton" mnemonicParsing="false" onAction="#returnAbout" styleClass="transparent-button" text="About">
                           <font>
                              <Font name="Lucida Sans Unicode" size="30.0" />
                           </font>
                        </Button>
                        <Button fx:id="branchesButton" mnemonicParsing="false" onAction="#returnBranches" styleClass="transparent-button" text="Branches">
                           <font>
                              <Font name="Lucida Sans Unicode" size="30.0" />
                           </font>
                        </Button>
                        <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="loginButton1" mnemonicParsing="false" onAction="#returnLogin" styleClass="transparent-button" text="Login">
                           <font>
                              <Font name="Lucida Sans Unicode" size="30.0" />
                           </font>
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" prefHeight="200.0" spacing="10.0">
                     <children>
                        <Line endX="100.0" startX="100.0" startY="50.0" stroke="#86c69a" strokeLineCap="ROUND" strokeWidth="3.0" />
                        <Text fx:id="pageTitle" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="page-title" text="Home">
                           <font>
                              <Font name="Lucida Sans Unicode" size="40.0" />
                           </font>
                           <HBox.margin>
                              <Insets right="760.0" />
                           </HBox.margin>
                        </Text>
                     </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <padding>
                              <Insets left="30.0" />
                           </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </top>
   <center>
      <StackPane fx:id="nestedUI" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <StackPane maxHeight="-Infinity" prefHeight="100.0" styleClass="bottom-navbar" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" maxHeight="-Infinity" prefHeight="100.0" spacing="50.0" />
         </children>
      </StackPane>
   </bottom>
</BorderPane>
   </content>
</ScrollPane>
